{
  "package_version": 2,
  "version": "1.1.3",
  "name": "skeleton-viewer",
  "description": "i18n:tools.description",
  "main": "./dist/main/index.js",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.5",
